{
    "manifest_version": 3,
    "name": "Capital Crunch",
    "version": "1.0",
    "description": "Test Your Knowledge on World Capitals!",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["alarms", "storage"],
    "action": {
        "default_popup": "options.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
  }
  